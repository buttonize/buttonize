{
	"extends": "@tsconfig/node20/tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "module": "NodeNext",
    "target": "esnext",
    "moduleResolution": "NodeNext",
    "outDir": "./dist",	
    "alwaysStrict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
  },
  "include": [
    "src",
  ],
  "exclude": []
}
