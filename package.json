{
	"name": "buttonize",
	"version": "0.0.1",
	"module": "cdk/index.js",
	"types": "cdk/index.d.ts",
	"license": "MIT",
	"type": "module",
	"author": {
		"name": "Buttonize",
		"url": "https://buttonize.io",
		"organization": "Buttonize"
	},
	"homepage": "https://buttonize.io",
	"bin": {
		"buttonize": "dist/cli/index.js"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:buttonize/buttonize.git"
	},
	"bugs": {
		"url": "https://github.com/buttonize/buttonize/issues"
	},
	"scripts": {
		"prepare": "husky install",
		"build": "rm -rf dist && tsc --project tsconfig.release.json && tsup cli/api/_ws.ts -d dist/cli --tsconfig=tsconfig.release.json --dts-only && cp package.json dist",
		"build:watch": "tsc --project tsconfig.release.json --watch",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"lint": "eslint 'src' 'test' --quiet --ext .ts",
		"lint:fix": "pnpm run lint --fix",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [
		"aws",
		"aws-cdk",
		"buttonize",
		"cdk",
		"serverless"
	],
	"engines": {
		"node": ">= 20"
	},
	"publishConfig": {
		"access": "public",
		"directory": "dist"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx}": "eslint --quiet --fix"
	},
	"devDependencies": {
		"@aws-sdk/client-sns": "^3.414.0",
		"@commitlint/cli": "^16.0.0",
		"@commitlint/config-conventional": "^16.0.0",
		"@tsconfig/node20": "^20.1.2",
		"@types/aws-lambda": "^8.10.109",
		"@types/node": "^18",
		"@types/traverse": "^0.6.36",
		"@typescript-eslint/eslint-plugin": "^6",
		"@typescript-eslint/parser": "^6",
		"@vitest/coverage-v8": "^1.2.2",
		"aws-cdk-lib": "2.95.1",
		"constructs": "10.0.5",
		"eslint": "^8",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-node": "^0.3.6",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"husky": "^7.0.4",
		"lint-staged": "^11.2.6",
		"prettier": "^2.5.1",
		"semantic-release": "^21.1.1",
		"typescript": "5.x",
		"vitest": "^1.2.2",
		"@types/ink-spinner": "^3.0.1",
		"@types/react": "^18.2.19",
		"@types/ws": "^8.5.5",
		"@types/yargs": "^17.0.24",
		"aws-cdk": "^2.93.0",
		"aws-sdk": "^2.1445.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-react": "^7.31.8",
		"tsup": "^8.0.2",
		"@types/symlink-or-copy": "^1.2.0"
	},
	"peerDependencies": {
		"aws-cdk-lib": ">= 2.95.1 < 3",
		"constructs": ">= 10.0.5 < 11"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.478.0",
		"@aws-sdk/client-lambda": "^3.478.0",
		"traverse": "^0.6.8",
		"@baselime/paginate-aws": "^1.0.6",
		"@trpc/server": "10.38.1",
		"cfn-resolver-lib": "^1.1.8",
		"chalk": "^5.3.0",
		"colorette": "^2.0.20",
		"cpx2": "^5.0.0",
		"get-port": "^7.0.0",
		"ink": "^4.3.1",
		"ink-link": "^3.0.0",
		"ink-spinner": "^5.0.0",
		"log-symbols": "^5.1.0",
		"ora": "^7.0.1",
		"react": "^18.2.0",
		"symlink-or-copy": "^1.3.1",
		"ws": "^8.13.0",
		"yargs": "^17.7.2"
	}
}
